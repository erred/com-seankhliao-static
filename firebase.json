{
  "hosting": {
    "site": "com-seankhliao-static",
    "public": "public/",
    "headers": [
      {
        "source": "*",
        "headers": [{ "key": "Strict-Transport-Security", "value": "max-age=63072000; preload" }]
      },
      {
        "source": "*.css",
        "headers": [{ "key": "Cache-Control", "value": "max-age=86400" }]
      },
      {
        "source": "*.woff2",
        "headers": [{ "key": "Cache-Control", "value": "max-age=2592000" }]
      },
      {
        "source": "*.(jpg|jpeg|webp|png|ico)",
        "headers": [{ "key": "Cache-Control", "value": "max-age=2592000" }]
      }
    ]
  }
}
